@model IEnumerable<OnlineHelpDesk.Areas.Admin.Models.RegisterUserModel>

@{
    ViewData["Title"] = "Acount List";
}
<h1 style="text-align:center">Account list</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Gender)
            </th>

         

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PhoneNumber)
                </td>
                <td>
                    @if (item.Gender)
                    {
                        <span>Male</span>
                    }
                    else
                    {
                        <span>Fmale</span>

                    }
                </td>
             
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id"><Button class="btn btn-success">Edit</Button></a>

                    <a asp-action="Delete" asp-route-id="@item.Id"><Button class="btn btn-danger">Delete</Button></a>


                </td>
            </tr>
        }
    </tbody>
</table>

