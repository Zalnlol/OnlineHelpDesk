@model IEnumerable<OnlineHelpDesk.Areas.Admin.Models.RequestSample>

@{
    ViewData["Title"] = "RequestSampleList";
}

<h1>RequestSampleList</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FacilityCategoryId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Content)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FacilityCategoryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Content)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RequestSampleId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RequestSampleId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RequestSampleId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
